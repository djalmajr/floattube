{
  "manifest_version": 2,
  "name": "Gittu Youtube",
  "version": "2.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": ["https://*.youtube.com/*","webview","app.window.alwaysOnTop"],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
